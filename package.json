{
    "private": true,
    "scripts": {
        "script-dev": "npm run script-development",
        "style-dev": "npm run style-development",
        "script-development": "cross-env NODE_ENV=development process.env.section=script node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "style-development": "cross-env NODE_ENV=development process.env.section=style node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run script-development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "script-prod": "npm run script-production",
        "style-prod": "npm run style-production",
        "script-production": "cross-env NODE_ENV=production process.env.section=script node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "style-production": "cross-env NODE_ENV=production process.env.section=style node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.19",
        "deepmerge": "^4.2.2",
        "fibers": "^5.0.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.19",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11",
        "vuetifyjs-mix-extension": "0.0.14"
    },
    "dependencies": {
        "echarts": "^4.8.0",
        "js-cookie": "^2.2.1",
        "laravel-echo": "^1.8.0",
        "moment": "^2.26.0",
        "nprogress": "^0.2.0",
        "socket.io-client": "^2.3.0",
        "vee-validate": "^3.2.5",
        "vue": "^2.6.11",
        "vue-advanced-chat": "^0.4.3",
        "vue-i18n": "^8.18.2",
        "vue-phone-number-input": "^1.1.10",
        "vue-road": "^1.0.5",
        "vue-router": "^3.1.6",
        "vuetify": "^2.2.17",
        "vuex": "^3.1.3",
        "webpack-cli": "^3.3.11"
    }
}
